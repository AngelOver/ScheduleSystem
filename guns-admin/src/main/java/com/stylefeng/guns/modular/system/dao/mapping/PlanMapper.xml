        <result column="remindtype" property="remindtype" />
        <result column="importantstatus" property="importantstatus" />
        <result column="is_wholeday" property="isWholeday" />
        <result column="is_repeat" property="isRepeat" />
        <result column="repeatcount" property="repeatcount" />
        <result column="is_progress" property="isProgress" />
        <result column="nowprogress" property="nowprogress" />
        <result column="createtime" property="createtime" />
        <result column="updatetime" property="updatetime" />
        <result column="version" property="version" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, userid, title, remark, address, starttime, endtime, remindtime, remindtype, importantstatus, is_wholeday AS isWholeday, is_repeat AS isRepeat, repeatcount, is_progress AS isProgress, nowprogress, createtime, updatetime, version
    </sql>

</mapper>
